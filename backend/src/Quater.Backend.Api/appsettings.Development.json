{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Redis": {
    "ConnectionString": "localhost:6379,abortConnect=false,connectTimeout=5000,syncTimeout=5000"
  },
  "RateLimiting": {
    "AuthenticatedLimit": 100,
    "AnonymousLimit": 20,
    "WindowSeconds": 60
  },
  "APIConfig": {
    "RateLimitingCleanupIntervalInMinutes": 5,
    "RateLimitingWindowInMinutes": 1,
    "RateLimitingMaxRequests": 60
  }
}
